	<!-- Shoping Cart -->
	<form class="bg0 p-t-75 p-b-85">
		<div class="container">
			<div class="row">
				<div class="col-lg-10 col-xl-7 m-lr-auto m-b-50">
					<div class="m-l-25 m-r--38 m-lr-0-xl">
						<div class="wrap-table-shopping-cart">
							<table class="table-shopping-cart">
								<tr class="table_head">
									<th class="column-1">Product</th>
									<th class="column-2"></th>
									<th class="column-3">Price</th>
									<th class="column-4"></th>
									<th class="column-5"></th>
								</tr>
								
								{{#each userWisLists}}

									<tr class="table_row" id="{{this.products._id}}wishList-row">
										<td class="column-1">
											<div class="how-itemcart1">
												<img src="{{this.products.image.[0].iamge_url}}" alt="IMG">
											</div>
										</td>
										<td class="column-2">{{this.products.title}}</td>
										<td class="column-3">&#x20B9;{{this.products.price}}</td>
										<td class="column-4">
											<button type="button" class="flex-c-m stext-101 cl0 size-101 bg3 bor14 hov-btn3 p-lr-15 trans-04 pointer" data-bs-toggle="modal"
												data-bs-target="#exampleModalwe{{this.products._id}}">Move to cart</button>
										</td>
									<td class="column-5"><i class="zmdi zmdi-close" onclick="deleteFromWishList('{{this.products._id}}','{{this._id}}')"></i></td>
										<td class="column-5"></td>
									</tr>

									{{!-- SIZE MODAL --}}
									<div class="modal " id="exampleModalwe{{this.products._id}}" tabindex="-1"
										role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
										<div class="modal-dialog modal-dialog-centered" role="document">
											<div class="modal-content">
												<form action="/v1/add-to-cart-from-wishlist/{{this.products._id}}" method="post" id="formwishlist">
													<div class="modal-header">
														<h5 class="modal-title" id="exampleModalLabel">
															{{this.products.title}}</h5>
														<button type="button" class="close" data-bs-dismiss="modal"
															aria-label="Close">
															<span aria-hidden="true">&times;</span>
														</button>
													</div>
													<div class="modal-body">
														{{!-- <input type="text" name="saf" id=""> --}}


														<select class="form-select form-select-sm"
															aria-label=".form-select-sm example" name="sizeId">
															<option >Open this select menu</option>
															{{#each this.sizes}}
																<option value="{{this._id}}">{{this.size}}</option>
															{{/each}}

														</select>

													</div>
													<div class="modal-footer">
														<button type="button" class="btn btn-danger"
															data-bs-dismiss="modal">Close</button>
														<input type="submit" class="btn btn-primary" value="Add To
															Cart">
													</div>
												</form>

											</div>
										</div>
									</div>


								{{/each}}


							</table>
						</div>


					</div>
				</div>


			</div>
		</div>
	</form>




